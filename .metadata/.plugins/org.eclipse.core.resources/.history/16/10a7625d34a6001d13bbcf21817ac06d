package ex5_2;

import java.util.*;

public class Ex5_2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Introduzca dos numeros:");
		int num1, num2, diff;
		int res = 0;
		num1 = sc.nextInt();
		num2 = sc.nextInt();
		diff = ((int)Math.max(num1, num2) - (int)Math.min(num1, num2));
		for (int i = 0; i < diff; i++) {
			res = (int)Math.min(num1, num2) + res;
		}
	}
}
